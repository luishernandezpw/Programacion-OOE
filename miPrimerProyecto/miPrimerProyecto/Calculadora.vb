Public Class Calculadora
    Public num1, num2 As Double
    Public Function sumar()
        Return num1 + num2
    End Function
    Public Function restar()
        Return num1 - num2
    End Function
    Public Function multiplicar()
        Return num1 * num2
    End Function
    Public Function dividir()
        Return num1 / num2
    End Function

End Class
